    <None Remove="DotR\**" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Remove="Trancity.Editor.resx" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="DotR">
      <HintPath>DotR\bin\Debug\DotR.dll</HintPath>
    </Reference>
    <Reference Include="SlimDX">
      <HintPath>..\Games\Trancity_0.6.4.3\SlimDX.dll</HintPath>
    </Reference>
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
  </ItemGroup>
</Project>